<table>
  <thead>
  <tr>
    <td>Trx_id</td>
    <td>click_key</td>
    <td>uid_hash</td>
    <td>advertising_id</td>
    <td>client_platform_type</td>
    <td>campaign_id</td>
    <td>ad_id</td>
    <td>event_code</td>
    <td>ad_title</td>
    <td>event_title</td>
    <td>reward</td>
    <td>language</td>
    <td>country</td>
    <td>issued_key</td>
    <td>적립시간</td>
  </tr>
  </thead>
  <tbody>
  <% user_rewards.each do |ur| %>
    <tr>
      <td><%= ur.trx_id %></td>
      <td><%= ur.click_key %></td>
      <td><%= ur.uid_hash %></td>
      <td><%= ur.advertising_id %></td>
      <td><%= ur.client_platform_type %></td>
      <td><%= ur.campaign_id %></td>
      <td><%= ur.ad_id %></td>
      <td><%= ur.event_code %></td>
      <td><%= ur.ad_title %></td>
      <td><%= ur.event_title %></td>
      <td><%= ur.reward %></td>
      <td><%= ur.language %></td>
      <td><%= ur.country %></td>
      <td><%= ur.issued_key %></td>
      <td><%= ur.issued_reward_at %></td>
    </tr>
  <% end %>
  </tbody>
</table>
